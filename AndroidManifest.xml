<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.timefliesagain"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="11"
        android:targetSdkVersion="23" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:label="@string/app_name" android:name="ToDoList"/>
        <activity android:label="@string/app_name" android:name="MyNotes"/>
        <activity android:label="@string/app_name" android:name="Questionnaire"/>
        <activity android:label="@string/app_name" android:name="StudyData"/>
        <activity android:label="@string/app_name" android:name="ListViewDraggingAnimation"/>
    </application>

</manifest>
